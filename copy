        AND A.POS_RGSTR_ID=B.POS_RGSTR_ID
        AND A.SHIFT_NO=B.SHIFT_NO
        AND A.SHPG_TXN_ID=B.SHPG_TXN_ID
        AND A.TXN_RPT_AS_DT >= '2022-05-01'
        AND A.TXN_RPT_AS_DT < '2022-06-01'
