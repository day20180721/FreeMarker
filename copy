        AND A.TXN_RPT_AS_DT >= '2023-05-01'
        AND A.TXN_RPT_AS_DT < '2023-06-01'
        AND A.PK_TYPE_CD = 3                       -- 3.ec
        AND C.DIVISION_CD != 9                     -- 第9處(EX.運費)不列入計算
