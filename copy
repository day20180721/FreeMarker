        AND A.TXN_RPT_AS_DT >= '2022-03-01'
        AND A.TXN_RPT_AS_DT < '2022-04-01'
        AND A.PK_TYPE_CD = 3                       -- 3.ec
        AND C.DIVISION_CD != 9                     -- 第9處(EX.運費)不列入計算
